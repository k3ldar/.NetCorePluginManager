@model DownloadPlugin.Models.DownloadModel
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer Localizer
@{
ViewData["Title"] = "Products";
}
<div class="leftColumn">
<h5 id="subHeader">@Localizer[nameof(Languages.LanguageStrings.Categories)]</h5>
<ul>
@foreach (var item in Model.Categories)
{
<li><a href="/Download/@item.Id/Category/@item.Name">@item.Name</a></li>
}
</ul>
<ul id="subOptions" visible="false"></ul>
<div id="subOptionsOther" visible="false"></div>
</div>
